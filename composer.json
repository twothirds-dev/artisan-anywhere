{
    "name": "two-thirds/artisan-anywhere",
    "description": "",
    "keywords": [],
    "homepage": "https://www.gitlab.com/two-thirds/artisan-anywhere.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Hoff",
            "email": "jhoff@twothirds.io"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.*|6.*",
        "nunomaduro/collision": "^2.1",
        "orchestra/testbench": "3.*|4.*",
        "symfony/console": "^3.2|4.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "~7.4"
    },
    "config": {
        "secure-http": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "TwoThirds\\ArtisanAnywhere\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwoThirds\\Testing\\": "tests"
        }
    }
}
